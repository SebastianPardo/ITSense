@{
    ViewBag.Title = "Eliminar";
}

<div class="container" style="margin-top:20px">
    <div class="row">
        <div class="col-md-3">
            @Html.Label("Codigo", new { @class = "control-label" })
            @Html.Editor("CodeDel", new { htmlAttributes = new { @class = "form-control", type = "number" } })
        </div>
    </div>
    <div class="row" style="margin-top:20px">
        <div class="col-md-3">
            <button type="button" class="btn btn-primary" id="btnDelete">Eliminar</button>
        </div>
    </div>
</div>
